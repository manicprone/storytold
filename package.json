{
  "name": "storytold",
  "version": "0.0.0",
  "description": "Map the chapters of your life into stories to tell.",
  "author": "|M| <manicprone@gmail.com>",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:manicprone/storytold.git"
  },
  "scripts": {
    "dev": "babel-node ./server.js",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "lint": "eslint src --cache --ignore-pattern .gitignore",
    "test": "cross-env NODE_ENV=test mocha --require babel-register test/**/*.spec.js",
    "db-init": "node db/init.js",
    "knex": "babel-node ./node_modules/.bin/knex --knexfile ./db/settings.js",
    "seed:system": "cross-env NODE_ENV=development SEED_DATA=system_data npm run knex seed:run",
    "seed:demo": "cross-env NODE_ENV=development SEED_DATA=demo_data npm run knex seed:run",
    "migrate:make": "npm run knex migrate:make",
    "migrate:latest": "NODE_ENV=development npm run knex migrate:latest",
    "migrate:rollback": "NODE_ENV=development npm run knex migrate:rollback",
    "migrate:reset-demo": "npm run migrate:rollback && npm run migrate:latest && npm run seed:system && npm run seed:demo"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "body-parser": "^1.17.2",
    "bookshelf": "^0.10.4",
    "compression": "^1.6.2",
    "connect-session-knex": "^1.3.4",
    "cookie-parser": "^1.4.3",
    "es6-promise": "^4.1.0",
    "express": "^4.15.2",
    "express-session": "^1.15.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "joint-kit": "0.0.18",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "lru-cache": "^4.0.2",
    "moment": "^2.18.1",
    "pg": "^7.0.1",
    "pg-escape": "^0.2.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.1",
    "serve-favicon": "^2.4.1",
    "vue": "^2.4.1",
    "vue-router": "^2.7.0",
    "vue-server-renderer": "^2.4.1",
    "vuetify": "^0.15.7",
    "vuex": "^2.3.1",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.2.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "glob": "^7.1.1",
    "knex-db-manager": "^0.3.0",
    "mocha": "^3.4.2",
    "rimraf": "^2.6.1",
    "sqlite3": "^3.1.9",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "sw-precache-webpack-plugin": "^0.10.1",
    "url-loader": "^0.5.8",
    "vue-loader": "^13.0.1",
    "vue-style-loader": "^3.0.0",
    "vue-template-compiler": "^2.4.1",
    "webpack": "^3.5.4",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.0.0",
    "webpack-node-externals": "^1.5.4"
  }
}
